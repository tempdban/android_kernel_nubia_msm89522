		broadcom_bcm2079x_nfc@0x76 {
			compatible = "broadcom,bcm2079x_nfc";
			reg = <0x76>;
			broadcom,irq-gpio = <&msm_gpio 37 0x00>;
			broadcom,en-gpio = <&msm_gpio 41 0x00>;
			broadcom,wake-gpio = <&msm_gpio 94 0x00>;
