&pmx_mdss_en {
	qcom,num-grp-pins = <1>;
	qcom,pins = <&gp 33>;
};

&pmx_mdss_te {
	qcom,num-grp-pins = <1>;
	qcom,pins = <&gp 24>;
};

&mdss_dsi0 {
	pinctrl-names = "mdss_default", "mdss_sleep";
	pinctrl-0 = <&mdss_dsi_rst_active  &mdss_dsi_en_active  &mdss_te_active>;
	pinctrl-1 = <&mdss_dsi_rst_suspend &mdss_dsi_en_suspend &mdss_te_suspend>;
	qcom,dsi-pref-prim-pan = <&dsi_otm1901a_1080p_video>;

	//qcom,platform-enable-gpio = <&msm_gpio 33 0>;
	qcom,platform-reset-gpio = <&msm_gpio 0 0>;
	//qcom,platform-bklight-en-gpio = <&msm_gpio 91 0>;
};

&dsi_otm1901a_1080p_video {
	qcom,mdss-dsi-lp11-init;
	qcom,mdss-dsi-init-delay-us = <20000>;
	qcom,cont-splash-enabled;
	qcom,panel-supply-entries = <&dsi_panel_pwr_supply>;
};

&dsi_hx8399a_1080p_video {
	qcom,mdss-dsi-lp11-init;
	qcom,mdss-dsi-init-delay-us = <20000>;
	qcom,cont-splash-enabled;
	qcom,panel-supply-entries = <&dsi_panel_pwr_supply>;
};

